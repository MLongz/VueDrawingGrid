{
  "name": "vue-drawing-grid",
  "version": "0.1.6",
  "description": "A simple Vue 3 drawing grid using HTML5 canvas",
  "author": {
    "name": "Long Huynh",
    "url": "https://github.com/MLongz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MLongz/VueDrawingGrid"
  },
  "bugs": {
    "url": "https://github.com/MLongz/VueDrawingGrid/issues"
  },
  "keywords": [
    "vue",
    "vue drawing grid",
    "drawing grid",
    "drawing"
  ],
  "private": false,
  "main": "dist/vue-drawing-grid.ssr.js",
  "browser": "dist/vue-drawing-grid.esm.js",
  "module": "dist/vue-drawing-grid.esm.js",
  "unpkg": "dist/vue-drawing-grid.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.50.0",
    "sass-loader": "^10.2.1",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "webpack": "^4.39.3"
  }
}
